    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "1981-05-20-anotaciones-en-espa-ol-canvas-1-tbd-annotation-3.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Todos juntos seremos la historia a cumplir, a cumplir, a cumplir. Venceremos, venceremos, mil cadenas habrá que romper. Venceremos, venceremos, la miseria sabremos vencer. Venceremos, venceremos, mil cadenas habrá que romper. Venceremos, venceremos, la miseria sabremos vencer.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-test/1981-05-20-anotaciones-en-espa-ol/canvas-1/canvas",
        "selector": {
